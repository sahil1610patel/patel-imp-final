{
  "family": "wildrydes-taskdef",
  "executionRoleArn": "<EXECUTION_ROLE_ARN>",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "wildrydes-webapp",
      "image": "<IMAGE_NAME>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [ "FARGATE" ],
  "cpu": "256",
  "memory": "512"
}
